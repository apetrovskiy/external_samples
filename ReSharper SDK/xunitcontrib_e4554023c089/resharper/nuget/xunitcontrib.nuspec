<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>xunitcontrib</id>
    <title>xUnit.net Test Support</title>
    <version>1.3.0</version>
    <authors>Matt Ellis</authors>
    <owners>Matt Ellis</owners>
    <description>A unit test provider for xUnit.net. Discovers and runs xUnit.net tests. Includes annotations to aid ReSharper inspections and Live Templates to speed up inserting test methods and asserts</description>
    <summary>A unit test provider for xUnit.net</summary>
    <releaseNotes>Traits are now applied to theories. Support for ReSharper 8.1</releaseNotes>
    <projectUrl>https://xunitcontrib.codeplex.com/</projectUrl>
    <licenseUrl>https://xunitcontrib.codeplex.com/license</licenseUrl>
    <iconUrl>http://download-codeplex.sec.s-msft.com/Download?ProjectName=xunit&amp;DownloadId=662625</iconUrl>
    <copyright>Copyright 2013 Matt Ellis</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="ReSharper" version="[8.0,8.2)" />
    </dependencies>
    <tags>resharper unittest xunit</tags>
  </metadata>
  <files>
    <!-- 8.0 plugin -->
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunitcontrib.runner.resharper.provider.8.0.dll"
          target="ReSharper\v8.0\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunitcontrib.runner.resharper.runner.8.0.dll"
          target="ReSharper\v8.0\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.dll"
          target="ReSharper\v8.0\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.runner.utility.dll"
          target="ReSharper\v8.0\plugins" />

    <!-- 8.1 plugin -->
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunitcontrib.runner.resharper.provider.8.1.dll"
          target="ReSharper\v8.1\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunitcontrib.runner.resharper.runner.8.1.dll"
          target="ReSharper\v8.1\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.dll"
          target="ReSharper\v8.1\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.runner.utility.dll"
          target="ReSharper\v8.1\plugins" />

    <!-- Settings and annotations -->
    <file src="..\src\xunitcontrib.runner.resharper.provider\templates.dotSettings"
          target="ReSharper\vAny\settings" />
    <file src="..\ExternalAnnotations\xunit.xml"
          target="ReSharper\vAny\annotations" />
  </files>
</package>

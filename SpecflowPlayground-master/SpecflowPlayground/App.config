<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="serviceConfiguration" type="SpecflowPlayground.Proxy.ServiceConfigurationSection, SpecflowPlayground" />
    
  <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" /></configSections>
  <specFlow>
    <language feature="en-US" />
    <unitTestProvider name="nUnit" />
    <runtime detectAmbiguousMatches="true" stopAtFirstError="false" missingOrPendingStepsOutcome="Error" />
    <trace traceSuccessfulSteps="true" traceTimings="false" minTracedDuration="0:0:0.1" />
    <plugins>
    </plugins>
    <stepAssemblies>
    </stepAssemblies>
  <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config --></specFlow>
  <serviceConfiguration>
    <serviceProxies>
      <serviceProxy name="subscriber" baseUrl="https://services.qa1.cdops.net/Subscriber" />
    </serviceProxies>
  </serviceConfiguration>
</configuration>
